{
  "name": "angular1.5-starter",
  "version": "1.5.11",
  "description": "An Angular project starter, pre-wired with Browserify and Gulp",
  "main": "index.js",
  "scripts": {
    "prestart": "gulp",
    "start": "http-server",
    "image:build": "gulp && docker build . -t duluca/angular1.5-starter",
    "image:run": "docker run -p 8080:3000 duluca/angular1.5-starter",
    "image:publish": "docker push duluca/angular1.5-starter"
  },
  "repository": {
    "type": "git",
    "url": "https://duluca@github.com/duluca/angular1.5-starter.git"
  },
  "keywords": [
    "Angular",
    "Gulp",
    "Browserify",
    "Router"
  ],
  "author": "Doguhan Uluca <duluca@gmail.com> (http://techtalkdc.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/duluca/angular1.5-starter/issues"
  },
  "homepage": "https://github.com/duluca/angular1.5-starter",
  "dependencies": {
    "angular": "~1.5.11",
    "angular-animate": "~1.5.11",
    "angular-aria": "~1.5.11",
    "angular-cookies": "^1.5.11",
    "angular-material": "~1.1.3",
    "angular-material-data-table": "~0.10.10",
    "angular-messages": "~1.5.11",
    "angular-resource": "~1.5.11",
    "lodash": "^4.6.1",
    "mdi": "^1.8.36",
    "ngcomponentrouter": "^2.1.0"
  },
  "devDependencies": {
    "browserify": "^14.1.0",
    "gulp": "^3.9.1",
    "gulp-insert": "^0.5.0",
    "gulp-jshint": "^2.0.4",
    "gulp-load-plugins": "^1.5.0",
    "gulp-ng-templates": "0.0.10",
    "gulp-uglify": "^2.1.0",
    "gulp-watch": "^4.3.11",
    "jshint": "^2.9.4",
    "merge2": "^1.0.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-paths": "^2.1.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^3.5.4"
  }
